{
  "name": "@proximacapital/zzz-repo-name-zzz",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:proximacapital/zzz-repo-name-zzz.git"
  },
  "homepage": "https://github.com/proximacapital/zzz-repo-name-zzz/blob/prod/README.md",
  "engines": {
    "node": "^16.6.1",
    "npm": ">=7.20.0"
  },
  "prepublish": "npm run build",
  "files": [
    "Dist"
  ],
  "ava": {
    "extensions": [
      "ts",
      "js"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "c8": {
    "check-coverage": false,
    "all": true,
    "include": [
      "**/*.ts"
    ],
    "exclude": [
      "coverage/**",
      "node_modules/**",
      "Dist/**",
      "**/*.test.ts",
      "**/*.demo.ts",
      "**/*.perf.ts",
      "**/*.network.ts",
      "**/*.data.ts"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ]
  },
  "scripts": {
    "reinstall": "rm -rf node_modules package-lock.json && npm i",
    "compile": "gulp build",
    "build": "gulp build",
    "lint": "gulp lint",
    "test": "gulp test",
    "start": "node Dist/Src/App.js"
  },
  "eslintConfig": {
    "extends": "./node_modules/@proxima-oss/eslint-config"
  },
  "dependencies": {
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@proxima-oss/eslint-config": "4.0.4",
    "@proximacapital/infrastructure": "4.3.1",
    "@types/lodash": "4.14.176",
    "@typescript-eslint/eslint-plugin": "5.2.0",
    "@typescript-eslint/eslint-plugin-tslint": "5.2.0",
    "@typescript-eslint/parser": "5.2.0",
    "ava": "3.15.0",
    "c8": "7.10.0",
    "del": "6.0.0",
    "eslint": "8.1.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-import-newlines": "1.1.5",
    "eslint-plugin-typescript-sort-keys": "2.0.0",
    "eslint-plugin-unicorn": "37.0.1",
    "gulp": "4.0.2",
    "markdownlint": "0.24.0",
    "markdownlint-cli": "0.29.0",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.11.0",
    "tslint": "6.1.3",
    "ttypescript": "1.5.12",
    "typescript": "4.4.4",
    "typescript-transform-paths": "3.3.1"
  }
}
