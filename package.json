{
  "name": "@proximacapital/zzz-repo-name-zzz",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:proximacapital/zzz-repo-name-zzz.git"
  },
  "homepage": "https://github.com/proximacapital/zzz-repo-name-zzz/blob/prod/README.md",
  "engines": {
    "node": "^16.6.1",
    "npm": ">=7.20.0"
  },
  "prepublish": "npm run build",
  "files": [
    "Dist"
  ],
  "scripts": {
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "compile": "npm run build",
    "build": "gulp build",
    "lint": "gulp lint",
    "test": "gulp test",
    "coverage": "gulp coverage"
  },
  "eslintConfig": {
    "parserOptions": {
      "project": [
        "./tsconfig.json"
      ],
      "sourceType": "module"
    },
    "extends": [
      "@proxima-oss/eslint-config/style/verbose"
    ]
  },
  "ava": {
    "extensions": [
      "ts",
      "js"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "c8": {
    "all": true,
    "include": [
      "Src"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ]
  },
  "dependencies": {
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@proxima-oss/eslint-config": "6.0.0",
    "@proxima-oss/gulp-tasks": "2.0.0",
    "@types/gulp": "4.0.9",
    "@types/lodash": "4.14.178",
    "ava": "4.0.1",
    "c8": "7.11.0",
    "del": "6.0.0",
    "gulp": "4.0.2",
    "markdownlint": "0.25.1",
    "markdownlint-cli": "0.31.1",
    "ts-node": "10.5.0",
    "tsconfig-paths": "3.12.0",
    "tslint": "6.1.3",
    "ttypescript": "1.5.13",
    "typescript": "4.8.4",
    "typescript-transform-paths": "3.3.1"
  },
  "volta": {
    "node": "16.6.1",
    "npm": "7.20.3"
  }
}
